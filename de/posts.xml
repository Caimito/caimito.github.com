layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Caimito Technologies - Deutsche Artikel</title>
 <link href="http://www.caimito.net/de/posts.xml" rel="self"/>
 <link href="http://www.caimito.net/de/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.caimito.net/de</id>
 <author>
   <name>Stephan Schwab</name>
   <email>sns@caimito.net</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.caimito.net{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.caimito.net{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
